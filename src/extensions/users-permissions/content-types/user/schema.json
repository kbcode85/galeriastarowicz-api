{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "email": {
      "type": "email",
      "unique": true,
      "required": true
    },
    "provider": {
      "type": "string"
    },
    "password": {
      "type": "password",
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false
    },
    "blocked": {
      "type": "boolean",
      "default": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"  
    },
    "phone": {
      "type": "string"
    },
    "company": {
      "type": "component",
      "component": "user.company",
      "repeatable": false
    },
    "address": {
      "type": "component", 
      "component": "user.address",
      "repeatable": false
    },
    "subscription": {
      "type": "component",
      "component": "subscription.user-subscription",
      "repeatable": false
    }
  }
} 